    "dotenv-cli": "1.4.0",
    "graphqlgen": "^0.6.0-rc9",
    "nodemon": "1.19.1",
    "prisma": "1.33.0",
    "rimraf": "2.6.3",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  }
